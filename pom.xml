                                </artifactItem>
                                <artifactItem>
                                    <groupId>xom</groupId>
                                    <artifactId>xom</artifactId>
                                    <version>${version.xom}</version>
                                </artifactItem>
                                <artifactItem>
                                    <groupId>org.jboss.as</groupId>
                                    <artifactId>jboss-as-cli</artifactId>
                                    <version>${jbossas-version}</version>
                                </artifactItem>
                                <artifactItem>
                                    <groupId>org.jboss.as</groupId>
                                    <artifactId>jboss-as-controller-client</artifactId>
                                    <version>${jbossas-version}</version>
                                </artifactItem>
                                <artifactItem>
                                    <groupId>org.jboss.as</groupId>
